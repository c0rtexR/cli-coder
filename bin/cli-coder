#!/usr/bin/env node

/**
 * CLI Coder executable script
 * This script serves as the entry point for the CLI tool
 */

// Import and run the main CLI program
import('../dist/index.js')
  .then(({ program }) => {
    // The program.parse() call is handled in the main module
  })
  .catch((error) => {
    console.error('Failed to start CLI Coder:', error.message);
    process.exit(1);
  });